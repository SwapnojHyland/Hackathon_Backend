          "feedbacks": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TestCaseFeedback"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "GenerateRequest": {
        "type": "object",
        "properties": {
          "jiraId": {
            "type": "string",
            "nullable": true
          },
          "summary": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "stepsToRecreate": {
            "type": "string",
            "nullable": true
          },
          "designNotes": {
            "type": "string",
            "nullable": true
          },
          "acceptanceCriteria": {
            "type": "string",
            "nullable": true
          },
          "testingRecommendations": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "TestCase": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "nullable": true
          },
          "given": {
            "type": "string",
            "nullable": true
          },
          "when": {
            "type": "string",
            "nullable": true
          },
          "then": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "TestCaseFeedback": {
        "type": "object",
        "properties": {
          "originalTitle": {
            "type": "string",
