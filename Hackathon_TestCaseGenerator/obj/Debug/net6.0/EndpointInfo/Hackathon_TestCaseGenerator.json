          },
          "summary": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "stepsToRecreate": {
            "type": "string",
            "nullable": true
          },
          "designNotes": {
            "type": "string",
            "nullable": true
          },
          "testingRecommendations": {
            "type": "string",
            "nullable": true
          },
          "acceptanceCriteria": {
            "type": "string",
            "nullable": true
          },
          "feedbackText": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UploadRequest": {
        "type": "object",
        "properties": {
          "jiraId": {
            "type": "string",
            "nullable": true
          },
          "testCases": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TestCase"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}